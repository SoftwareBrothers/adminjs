{
  "name": "adminjs",
  "version": "7.0.0-beta-v7.1",
  "description": "Admin panel for apps written in node.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.js"
    }
  },
  "scripts": {
    "test": "mocha --loader=ts-node/esm ./spec/index.js",
    "types": "tsc",
    "clean": "rm -rf lib && mkdir lib && rm -fr types && mkdir types",
    "build": "babel src --out-dir lib --copy-files --extensions '.ts,.js,.jsx,.tsx'",
    "lint": "eslint './spec/**/*' './src/**/*' './cy/**/*' './*'",
    "cover": "NODE_ENV=test nyc --reporter=lcov --reporter=text-lcov npm test",
    "codecov": "NODE_ENV=test nyc --reporter=text-lcov npm test | codecov --pipe",
    "bundle": "node bin/watch-dev.js",
    "bundle:globals": "node bin/bundle-globals.js",
    "cspell": "cspell src/**/*.ts src/**/*.js src/**/*.tsx src/**/*.jsx",
    "check:all": "yarn types && yarn cspell && yarn lint && yarn test && ONCE=true yarn bundle && ONCE=true NODE_ENV=production yarn bundle",
    "dev": "npm-run-all --parallel \"build --watch\" \"bundle\" \"types --watch\"",
    "release": "semantic-release"
  },
  "bin": {
    "admin": "./cli.js"
  },
  "nyc": {
    "exclude": [
      "spec",
      "example-app",
      "src/**/*.spec.ts",
      "src/**/*.spec.js",
      "src/**/*.spec.tsx",
      "src/**/*.spec.jsx",
      "src/**/*.factory.ts",
      "src/backend/bundler/user-components-bundler.ts",
      "docs",
      "coverage",
      "types",
      "src/frontend/assets/scripts",
      "lib",
      ".vscode",
      ".github",
      "**/*.spec.js"
    ],
    "all": true,
    "extension": [
      ".js",
      ".jsx",
      ".tsx",
      ".ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SoftwareBrothers/adminjs.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "hapi",
    "express",
    "mongoose",
    "admin",
    "admin-panel"
  ],
  "browserslist": [
    "last 5 Chrome versions"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Wojciech Krysiak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SoftwareBrothers/adminjs/issues"
  },
  "homepage": "https://github.com/SoftwareBrothers/adminjs#readme",
  "dependencies": {
    "@adminjs/design-system": "^4.0.0-beta-v4.1",
    "@babel/core": "^7.20.12",
    "@babel/parser": "^7.20.15",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@emotion/stylis": "^0.8.5",
    "@hello-pangea/dnd": "^16.2.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/babel-core": "^6.25.7",
    "@types/react": "^18.0.28",
    "axios": "^1.3.3",
    "babel-plugin-styled-components": "^2.0.7",
    "commander": "^10.0.0",
    "flat": "^5.0.2",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.1.1",
    "lodash": "^4.17.21",
    "ora": "^6.1.2",
    "prop-types": "^15.8.1",
    "punycode": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.5",
    "react-is": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.8.1",
    "react-router-dom": "^6.8.1",
    "redux": "^4.2.1",
    "rollup": "^3.15.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "slash": "^5.0.0",
    "styled-components": "6.0.0-beta.11",
    "uuid": "^9.0.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@emotion/is-prop-valid": "^1.2.0",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/react": "^13.4.0",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/factory-girl": "^5.0.8",
    "@types/flat": "^5.0.2",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@types/react-dom": "^18.0.11",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-change": "^2.1.2",
    "core-js": "^3.28.0",
    "cspell": "^6.26.1",
    "eslint": "^8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "factory-girl": "^5.0.4",
    "husky": "^8.0.3",
    "istanbul": "^0.4.5",
    "jsdom": "^21.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.2.0",
    "node-esm-import-all": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "semantic-release": "^20.1.0",
    "semantic-release-slack-bot": "^3.5.3",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0",
    "typescript": "^4.9.5"
  },
  "resolutions": {
    "styled-components": "6.0.0-beta.11",
    "react-redux": "8.0.5",
    "redux": "4.2.1"
  }
}
